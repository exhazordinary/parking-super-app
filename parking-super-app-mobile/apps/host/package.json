{
  "name": "@parking/host",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "react-native webpack-start --port 8081 --webpackConfig webpack.config.mjs",
    "build:ios": "react-native webpack-bundle --platform ios --dev false --entry-file index.js --webpackConfig webpack.config.mjs",
    "build:android": "react-native webpack-bundle --platform android --dev false --entry-file index.js --webpackConfig webpack.config.mjs",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "clean": "rm -rf dist build",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:test:ios:release": "detox test --configuration ios.sim.release",
    "e2e:test:android:release": "detox test --configuration android.emu.release"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-reanimated": "^3.8.0",
    "react-native-vector-icons": "^10.0.3",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.22",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "@tanstack/react-query": "^5.28.0",
    "zustand": "^4.5.2",
    "@sentry/react-native": "^5.19.0",
    "@parking/ui": "*",
    "@parking/offline": "*",
    "@parking/api": "*",
    "@parking/auth": "*",
    "@parking/navigation": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.24.0",
    "@callstack/repack": "^4.0.0",
    "@react-native/babel-preset": "^0.73.21",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.5",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.64",
    "@types/react-native": "^0.73.0",
    "babel-loader": "^9.1.3",
    "detox": "^20.18.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "react-native-svg": "^15.1.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2",
    "webpack": "^5.90.3"
  }
}
